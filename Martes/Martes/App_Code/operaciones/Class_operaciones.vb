Imports Microsoft.VisualBasic

Public Class Class_operaciones
    Public a, b, c As Integer

    Public Sub sub_suma()
        c = a + b

    End Sub


    Public Sub sub_resta()
        c = a - b


    End Sub


    Public Sub sub_div()
        c = a / b
    End Sub


    Public Sub sub_multi()
        c = a * b

    End Sub

End Class
